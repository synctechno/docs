msgstr ""

#: ../../source/manual/how-tos/insight.rst:128
msgid "FlowInterfaceTotals - Totals per interface"
msgstr ""

#: ../../source/manual/how-tos/insight.rst:129
msgid "FlowDstPortTotals - Totals per destination port"
msgstr ""

